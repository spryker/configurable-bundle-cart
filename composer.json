{
  "name": "spryker/configurable-bundle-cart",
  "type": "library",
  "description": "ConfigurableBundleCart module",
  "license": "proprietary",
  "require": {
    "php": ">=7.1",
    "spryker/cart": "^7.3.0",
    "spryker/cart-extension": "^1.6.0 || ^2.2.1 || ^4.0.0",
    "spryker/configurable-bundle": "^1.0.0",
    "spryker/kernel": "^3.33.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/testify": "*"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
