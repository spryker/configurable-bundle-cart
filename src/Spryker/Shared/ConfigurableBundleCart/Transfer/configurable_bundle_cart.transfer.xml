<?xml version="1.0"?>
<transfers xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xmlns="spryker:transfer-01"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="Item">
        <property name="configuredBundleItem" type="ConfiguredBundleItem" />
        <property name="configuredBundle" type="ConfiguredBundle" />
    </transfer>

    <transfer name="ConfiguredBundle">
        <property name="template" type="ConfigurableBundleCartTemplate" />
        <property name="quantity" type="int" />
        <property name="groupKey" type="string" />
    </transfer>

    <transfer name="ConfiguredBundleItem">
        <property name="slot" type="ConfigurableBundleCartTemplateSlot" />
    </transfer>

    <transfer name="ConfigurableBundleCartTemplate">
        <property name="idConfigurableBundleCartTemplate" type="int" />
        <property name="uuid" type="string" />
        <property name="name" type="string" />
    </transfer>

    <transfer name="ConfigurableBundleCartTemplateSlot">
        <property name="idConfigurableBundleCartTemplateSlot" type="int" />
        <property name="uuid" type="string" />
    </transfer>

</transfers>
